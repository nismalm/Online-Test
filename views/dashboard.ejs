                    <div class="card">
                      <ul class="list-group">
                        <li class="list-group-item d-flex justify-content-between align-items-center">
                          <h4 class="card-title">Questions</h4>
                        </li>
                        <% results.forEach(function(el){ %>
                          <li class="list-group-item d-flex justify-content-between align-items-center">
                            <%= (el._id).charAt(0).toUpperCase()+(el._id).slice(1) %>
